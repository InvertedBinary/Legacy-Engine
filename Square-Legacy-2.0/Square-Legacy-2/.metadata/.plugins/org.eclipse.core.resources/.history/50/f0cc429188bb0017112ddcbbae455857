public class MobFactory {
	
	public MobFactory() {
	}
	
	public void build(int id) {
		
		
	}
	
	
	
	
	
	
	
	
	public Item buildItem() {
		return new Item();
		
	}
	
	public Item buildItem(int id) {
		Item it = buildItem();
		
		switch(id) {
			
		}
		
		
		return it;
	}
	
	public Item buildItem(String name) {
		Item it = buildItem();
		return it;
		
	}
	
	
	public Item buildItem(int xp, int yp, Item item) {
		Item it = buildItem();
			  if (item instanceof HealthPot) {
			it = new HealthPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof ManaPot) {
			it = new ManaPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof StaminaPot) {
			it = new StaminaPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof OpticBond) {
			it = new OpticBond((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorHealth) {
			it = new RingOfMinorHealth((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorMana) {
			it = new RingOfMinorMana((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorStamina) {
			it = new RingOfMinorStamina((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof wand_ArcaneTwig) {
			it = new wand_ArcaneTwig((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_FlareScepter) {
			it = new wand_FlareScepter((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_VoidCrook) {
			it = new wand_VoidCrook((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_Pulsefire) {
			it = new wand_Pulsefire((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_StygianScepter) {
			it = new wand_StygianScepter((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_ContradictionWand) {
			it = new wand_ContradictionWand((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof CoinBag) {
				  item.clickEvent();
		}else if (item instanceof CottonRobe) {
			it = new CottonRobe((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof AbyssalArmor) {
			it = new AbyssalArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof DragonsArmor) {
			it = new DragonsArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof SunAcolyteCloak) {
			it = new SunAcolyteCloak((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof LinenRobes) {
			it = new LinenRobes((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof MaelstromRobes) {
			it = new MaelstromRobes((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof CopperArmor) {
			it = new CopperArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof IronArmor) {
			it = new IronArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof InvisPot) {
			it = new InvisPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof Stick) {
			it = new Stick((xp), (yp + 6), 7400, 1);
		}else if (item instanceof CopperIngot) {
			it = new CopperIngot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof IronIngot) {
			it = new IronIngot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof Leather) {
			it = new Leather((xp), (yp + 6), 7400, 1);
		}else if (item instanceof WyvernSkinArmor) {
			it = new WyvernSkinArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof LeatherTunic) {
			it = new LeatherTunic((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof StuddedLeatherTunic) {
			it = new StuddedLeatherTunic((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof ChaosTunic) {
			it = new ChaosTunic((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof Charm_ATC) {
			it = new Charm_ATC((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof Charm_AGI) {
			it = new Charm_AGI((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);

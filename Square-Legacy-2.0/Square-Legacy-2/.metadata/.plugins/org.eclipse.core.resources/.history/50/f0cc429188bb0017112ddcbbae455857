		}else if (item instanceof Charm_WIS) {
			it = new Charm_WIS((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof Charm_EDR) {
			it = new Charm_EDR((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof Charm_MDF) {
			it = new Charm_MDF((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof Charm_MAT) {
			it = new Charm_MAT((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof BrokenHilt) {
			it = new BrokenHilt((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof CubeOfBefuddlement) {
			it = new CubeOfBefuddlement((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof DiamondShard) {
			it = new DiamondShard((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof ObsidianShard) {
			it = new ObsidianShard((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof Pentagram) {
			it = new Pentagram((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof FrostflamePentagram) {
			it = new FrostflamePentagram((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof EmptyFlask) {
			it = new EmptyFlask((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);

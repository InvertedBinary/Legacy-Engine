			it = new TatteredCloth((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMajorMana) {
			it = new RingOfMajorMana((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMajorStamina) {
			it = new RingOfMajorStamina((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMajorHealth) {
			it = new RingOfMajorHealth((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof AbyssalBindingOoze) {
			it = new AbyssalBindingOoze((xp), (yp + 6), 7400, 1);
		}else if (item instanceof ArcaneEsscence) {
			it = new ArcaneEsscence((xp), (yp + 6), 7400, 1);
		}else if (item instanceof ArcanemIngot) {
			it = new ArcanemIngot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof BindingOoze) {
			it = new BindingOoze((xp), (yp + 6), 7400, 1);
		}else if (item instanceof EarthEsscence) {
			it = new EarthEsscence((xp), (yp + 6), 7400, 1);
		}else if (item instanceof EtherialBindingOoze) {
			it = new EtherialBindingOoze((xp), (yp + 6), 7400, 1);
		}else if (item instanceof FlameEsscence) {
			it = new FlameEsscence((xp), (yp + 6), 7400, 1);
		}else if (item instanceof SteelIngot) {
			it = new SteelIngot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof ThunderEsscence) {
			it = new ThunderEsscence((xp), (yp + 6), 7400, 1);
		}else if (item instanceof VoidEsscence) {
			it = new VoidEsscence((xp), (yp + 6), 7400, 1);
		}else if (item instanceof WaterEsscence) {
			it = new WaterEsscence((xp), (yp + 6), 7400, 1);
		}
		
		return it;
	}
	
}

		}else if (item instanceof wand_ContradictionWand) {
			it = new wand_ContradictionWand((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof CoinBag) {
				  item.clickEvent();
		}else if (item instanceof CottonRobe) {
			it = new CottonRobe((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);
		}else if (item instanceof AbyssalArmor) {
			it = new AbyssalArmor((xp), (yp + 6), 7400, 1, EquipableItem.slot_CHEST);

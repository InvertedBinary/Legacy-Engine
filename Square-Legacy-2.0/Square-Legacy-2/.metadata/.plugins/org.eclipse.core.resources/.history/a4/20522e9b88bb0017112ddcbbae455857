package com.IB.SL.entity.mob;


public class MobFactory {
	
	public MobFactory() {
	}
	
	public void build(int id) {
		
		
	}
	
	
	
	
	
	
	
	
	public Item buildItem() {
		return new Item();
		
	}
	
	public Item buildItem(int id) {
		Item it = buildItem();
		
		switch(id) {
			
		}
		
		
		return it;
	}
	
	public Item buildItem(String name) {
		Item it = buildItem();
		return it;
		
	}
	
	
	public Item buildItem(int xp, int yp, Item item) {
		Item it = buildItem();
			  if (item instanceof HealthPot) {
			it = new HealthPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof ManaPot) {
			it = new ManaPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof StaminaPot) {
			it = new StaminaPot((xp), (yp + 6), 7400, 1);
		}else if (item instanceof OpticBond) {
			it = new OpticBond((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorHealth) {
			it = new RingOfMinorHealth((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorMana) {
			it = new RingOfMinorMana((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof RingOfMinorStamina) {
			it = new RingOfMinorStamina((xp), (yp + 6), 7400, 1, EquipableItem.slot_UTILITY1);
		}else if (item instanceof wand_ArcaneTwig) {
			it = new wand_ArcaneTwig((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_FlareScepter) {
			it = new wand_FlareScepter((xp), (yp + 6), 7400, 1, EquipableItem.slot_WEAPON);
		}else if (item instanceof wand_VoidCrook) {
